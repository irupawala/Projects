////////////////////////////////// SHIFT LEFT FOR BRANCH ////////////////////////////////////////////

// Shiftleft2.vp 
//; use POSIX;
//; use strict;
//; use warnings FATAL => qw(all);
//; use diagnostics;



module `mname` ( 
  input logic [31:0] Shiftleft2_in,
  output logic [31:0] Shiftleft2_out
);
 
  always_comb
  begin
  Shiftleft2_out=Shiftleft2_in<<2;
  end

endmodule: `mname`

